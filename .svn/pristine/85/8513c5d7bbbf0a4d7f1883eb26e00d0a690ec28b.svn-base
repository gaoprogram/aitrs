@import "variable.styl"

.mg-30
  margin 30px

.el-switch__core:after {
  content: "";
  position: absolute;
  top: 1px;
  left: 1px;
  border-radius: 100%;
  transition: all .3s;
  width: 16px;
  height: 16px;
  background-color: #fff;
}

.el-switch.is-checked .el-switch__core:after {
  left: 100%;
  margin-left: -17px;
}

.table-design-container
  .el-scrollbar__wrap
    overflow-x hidden!important

.launch-container
  .el-scrollbar__wrap
    overflow-x: hidden !important

.el-dialog
  max-height 70vh
  overflow-y scroll
  .el-dialog__footer
    text-align center

.el-dialog.is-fullscreen {
  max-height 100%
  overflow: auto;
}

input::-ms-clear{
  display: none;
}

input::-webkit-search-cancel-button{
  display: none;
}
